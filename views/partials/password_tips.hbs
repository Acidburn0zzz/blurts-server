{{!< default }}
  <div class="section-wrapper tips" role="complementary">
    <section id="password-advice" class="whole">
      <h3 class="section-headline">{{ prototype.copy.passwordTips.headline }}</h3>
      {{#if prototype.copy.passwordTips.blurb}}
        <p class="pwt-blurb">{{ prototype.copy.passwordTips.blurb }}</p>
      {{/if}}
    </section>

    {{#each prototype.copy.passwordTips.tips }}
      <div class="tip third">
        <div class="icon tip-icon tip-icon-{{breachMath @index "+" 1}}"></div>
          <h4 class="medium">{{ headline }}</h4>
        <p>{{ subhead }}</p>
      </div>
   {{/each}}
  </div>
